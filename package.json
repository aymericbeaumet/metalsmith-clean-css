{
  "name": "metalsmith-clean-css",
  "version": "6.0.0",
  "description": "A Metalsmith plugin to minify CSS files",
  "keywords": [
    "metalsmith",
    "plugin",
    "css",
    "clean-css",
    "minifier"
  ],
  "homepage": "https://github.com/aymericbeaumet/metalsmith-clean-css",
  "repository": {
    "type": "git",
    "url": "https://github.com/aymericbeaumet/metalsmith-clean-css.git"
  },
  "license": "MIT",
  "author": {
    "name": "Aymeric Beaumet",
    "email": "hi@aymericbeaumet.com",
    "url": "https://aymericbeaumet.com"
  },
  "files": [
    "index.js"
  ],
  "scripts": {
    "format": "prettier --write \"{,.}**.{js,md,yml}\" && xo --fix",
    "release": "yarn test && np",
    "test": "prettier --check \"{,.}**.{js,md,yml}\" && xo && ava"
  },
  "dependencies": {
    "async": "3.1.0",
    "minimatch": "3.0.4"
  },
  "devDependencies": {
    "ava": "2.3.0",
    "clean-css": "4.2.1",
    "np": "5.0.3",
    "prettier": "1.18.2",
    "xo": "0.24.0"
  },
  "peerDependencies": {
    "clean-css": "^4.1.9"
  },
  "prettier": {
    "semi": false,
    "useTabs": false,
    "tabWidth": 2,
    "trailingComma": "es5",
    "singleQuote": true,
    "bracketSpacing": true,
    "jsxBracketSameLine": false
  },
  "xo": {
    "prettier": true
  }
}
